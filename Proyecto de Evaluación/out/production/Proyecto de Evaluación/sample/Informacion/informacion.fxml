<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="stackpane" maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Informacion.informacion">
    <children>
        <AnchorPane fx:id="anchorpane" maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" styleClass="bodybg" stylesheets="@../Estilos/estilos.css">
            <children>
                <HBox layoutX="1.0" prefHeight="69.0" prefWidth="818.0" AnchorPane.bottomAnchor="519.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <Label prefHeight="42.0" prefWidth="509.0" text="Información Acerca del Covid-19">
                            <font>
                                <Font name="Corbel Bold Italic" size="36.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="50.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                      <Label prefHeight="52.0" prefWidth="57.0" styleClass="tos2" />
                    </children>
                </HBox>
                <Label layoutX="75.0" layoutY="76.0" text="¿Qué es el Covid-19?" AnchorPane.leftAnchor="75.0">
                    <font>
                        <Font name="Corbel Bold" size="19.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="105.0" prefHeight="85.0" prefWidth="535.0" text="Los coronavirus son una familia de virus que causan enfermedades (desde el resfriado común hasta enfermedades respiratorias más graves) y circulan entre humanos y animales." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="174.0" prefHeight="85.0" prefWidth="515.0" text="En este caso, se trata del SARS-COV2. Apareció en China en diciembre pasado y provoca una enfermedad llamada COVID-19, que se extendió por el mundo y fue declarada pandemia global por la Organización Mundial de la Salud." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="75.0" layoutY="248.0" text="¿Cuáles son los síntomas?">
                    <font>
                        <Font name="Corbel Bold" size="19.0" />
                    </font>
                </Label>
                <Label layoutX="92.0" layoutY="256.0" prefHeight="60.0" prefWidth="718.0" text="Tos y/o fiebre y/o dolor de cabeza." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="92.0" layoutY="302.0" prefHeight="60.0" prefWidth="509.0" text="Y se acompaña de al menos uno de los siguientes: dolor o ardor de garganta, ojos rojos, dolores en músculos o articulaciones (malestar general)." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="92.0" layoutY="356.0" prefHeight="52.0" prefWidth="481.0" text="Los casos más graves tienen dificultades para respirar o falta de aire en sus pulmones." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="75.0" layoutY="408.0" text="¿A qué personas les afecta más?">
                    <font>
                        <Font name="Corbel Bold" size="19.0" />
                    </font>
                </Label>
                <Label layoutX="458.0" layoutY="437.0" prefHeight="85.0" prefWidth="93.0" styleClass="gifprueba" stylesheets="@estilos.css" />
                <Label layoutX="92.0" layoutY="425.0" prefHeight="60.0" prefWidth="718.0" text="A las personas mayores." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="92.0" layoutY="455.0" prefHeight="60.0" prefWidth="446.0" text="Las personas embarazadas." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="92.0" layoutY="485.0" prefHeight="85.0" prefWidth="294.0" text="las personas con alguna enfermedad previa como cáncer, diabetes o hipertensión." textAlignment="JUSTIFY" wrapText="true">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="70.0" prefHeight="35.0" prefWidth="35.0" styleClass="estrellaowo" stylesheets="@estilos.css" />
                <Label layoutX="23.0" layoutY="254.0" prefHeight="35.0" prefWidth="35.0" styleClass="estrellaowo" stylesheets="@estilos.css" />
                <Label layoutX="23.0" layoutY="402.0" prefHeight="35.0" prefWidth="35.0" styleClass="estrellaowo" stylesheets="@estilos.css" />
            <Button layoutX="21.0" layoutY="557.0" mnemonicParsing="false" onAction="#btinicio" text="Atras">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="degradado_botones_simulacion" />
                  <String fx:value="Redondo" />
               </styleClass>
            </Button>
            <Button layoutX="512.0" layoutY="557.0" mnemonicParsing="false" onAction="#irsiguiente" text="Siguiente">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="degradado_botones_simulacion" />
                  <String fx:value="Redondo" />
               </styleClass>
            </Button>
            </children>
        </AnchorPane>
    </children>
</StackPane>
